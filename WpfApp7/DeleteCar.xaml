<Window x:Class="WpfApp7.DeleteCar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp7"
        mc:Ignorable="d"
        Title="DeleteCar" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="Вы уверены, что хотите удалить автомобиль?" />
            <Button x:Name="btnYes" Content="Да" Click="btnYes_Click" />
            <Button x:Name="btnNo" Content="Нет" Click="btnNo_Click" />
        </StackPanel>
    </Grid>
</Window>
